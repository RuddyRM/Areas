@model Assignment3Test.Areas.ContactDetails.Models.ContactDetails

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@using (Html.BeginForm("Index", "Management"))
{
    <label>First Name</label>
    @Html.EditorFor(m => m.FirstName)
    <label>Last Name</label>
    @Html.EditorFor(m => m.LastName)
    <label>Email</label>
    @Html.EditorFor(m => m.Email)

    <h3>Cell Phone Details</h3>

    <label>Country Code</label>
    @Html.EditorFor(m => m.CellPhone.CountryCode)
    <label>Area Code</label>
    @Html.EditorFor(m => m.CellPhone.AreaCode)
    <label>Cell Number</label>
    @Html.EditorFor(m => m.CellPhone.PhoneNumber)

    <h3>Home Phone Details</h3>

    <label>Country Code</label>
    @Html.EditorFor(m => m.HomePhone.CountryCode)
    <label>Area Code</label>
    @Html.EditorFor(m => m.HomePhone.AreaCode)
    <label>Phone Number</label>
    @Html.EditorFor(m => m.HomePhone.PhoneNumber)

    <h3>Home Address Details</h3>

    <label>Country</label>
    @Html.EditorFor(m => m.HomeAddress.Country)
    <label>Province</label>
    @Html.EditorFor(m => m.HomeAddress.Province)
    <label>City</label>
    @Html.EditorFor(m => m.HomeAddress.City)
    <label>Address 1</label>
    @Html.EditorFor(m => m.HomeAddress.Line1)
    <label>Address 2</label>
    @Html.EditorFor(m => m.HomeAddress.Line2)
    <label>Address 3</label>
    @Html.EditorFor(m => m.HomeAddress.Line3)
    <label>Postal Code</label>
    @Html.EditorFor(m => m.HomeAddress.PostalCode)

    <br/>


    <input type="submit" value="Submit" />
}
